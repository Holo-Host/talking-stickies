{
  "name": "talking-stickies-app",
  "version": "0.1.0",
  "scripts": {
    "build": "rimraf public/build && rollup -c",
    "build:watch": "run-singleton \"rollup -c -w\"",
    "dev:watch": "echo \"starting ui instance: $HC_ID\" && run-singleton \"PORT=500$HC_ID SVELTE_APP_APP_ID=\"talking-stickies$HC_ID\" SVELTE_APP_APP_PORT=\"$HC_PORT\" rollup -c -w\"",
    "dev": "echo \"starting ui instance: $HC_ID\" && PORT=500$HC_ID SVELTE_APP_APP_ID=\"talking-stickies$HC_ID\" SVELTE_APP_APP_PORT=\"$HC_PORT\" rollup -c",
    "dev2": "PORT=5001 SVELTE_APP_APP_ID=talking-stickies2 SVELTE_APP_APP_PORT=8887 rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "run-singleton-cli": "^0.0.7",
    "watch-cli": "^0.2.3",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "typescript": "^3.9.3",
    "tslib": "^2.0.0",
    "@tsconfig/svelte": "^1.0.0"
  },
  "dependencies": {
    "@holochain/client": "0.7.0",
    "@holochain-open-dev/cell-client": "0.6.1",
    "@holochain-open-dev/core-types": "0.4.0",
    "@holochain-open-dev/utils": "0.2.4",
    "@holochain-syn/store": "0.0.4",
    "lodash": "^4.17.21",
    "sirv-cli": "^1.0.0",
    "svelte-fa": "^2.2.0",
    "uuid": "^8.3.2"
  }
