{
  "name": "talking-stickies-holo-app",
  "version": "0.6.2",
  "scripts": {
    "build:raw": "rimraf public/build && rollup -c",
    "build:dev": "SVELTE_APP_CHAPERONE_URL=https://chaperone-rsm.dev.holotest.net npm run build:raw",
    "build:QA": "SVELTE_APP_CHAPERONE_URL=https://chaperone-rsm.qa.holotest.net npm run build:raw",
    "build:alpha": "SVELTE_APP_CHAPERONE_URL=https://chaperone.holo.hosting npm run build:raw",
    "build:watch": "run-singleton \"rollup -c -w\"",
    "dev:watch": "echo \"starting ui UI_PORT=$UI_PORT APP_PORT=$APP_PORT\" && run-singleton \"PORT=$UI_PORT SVELTE_APP_APP_ID=\"talking-stickies$APP_PORT\" SVELTE_APP_APP_PORT=\"$APP_PORT\" rollup -c -w\"",
    "dev": "echo \"starting ui UI_PORT=$UI_PORT APP_PORT=$APP_PORT\" && run-singleton \"PORT=$UI_PORT SVELTE_APP_APP_ID=\"talking-stickies$APP_PORT\" SVELTE_APP_APP_PORT=\"$APP_PORT\" rollup -c\"",
    "dev2": "PORT=5001 SVELTE_APP_APP_ID=talking-stickies2 SVELTE_APP_APP_PORT=8887 rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "package": "echo \"packaging what was previously built in ./public\" && rimraf ui.zip && cd ./public && bestzip ../ui.zip * "
  },
  "dependencies": {
    "@holo-host/boardz": "0.8.0",
    "@holo-host/web-sdk": "^0.6.7-prerelease",
    "@holochain-open-dev/utils": "0.7.2",
    "@holochain-syn/core": "0.3.0-alpha.5",
    "@holochain/client": "0.11.15",
    "@ts-stack/markdown": "^1.4.0",
    "lodash": "^4.17.21",
    "sirv-cli": "^1.0.0",
    "svelte-fa": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.50.1",
    "run-singleton-cli": "^0.0.7",
    "watch-cli": "^0.2.3",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.10.7",
    "@rollup/plugin-typescript": "^6.0.0",
    "typescript": "^3.9.3",
    "tslib": "^2.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "bestzip": "^2.2.0"
  }
}
